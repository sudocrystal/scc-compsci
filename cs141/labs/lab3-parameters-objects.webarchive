// Rearranges the contents of the given array into random order.
function shuffle(array) {
    for (var i = 0; i < array.length - 1; i++) {
        var j = Math.floor(Math.random() * (array.length - i)) + i;
        var temp = array[i];
        array[i] = array[j];
        array[j] = temp;
    }
}



