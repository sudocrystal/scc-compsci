            if (!incrementals[snum] || incpos >= incrementals[snum].length) {
                go(1);       // next slide
            } else {
                subgo(1);
            }
        } else {
            if (!incrementals[snum] || incpos == 0) {
                go(-1);      // previous slide
            } else {
                subgo(-1);
            }
        }
        
        return false;
    }
